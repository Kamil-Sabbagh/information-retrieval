    const { product: { id  } ,  } = props;
    return shouldShowComponent ? defaultId || id : undefined;
}"><div id="rcpd14281" class=""><div class="listicleProduct dn-print mv8 listicle-product__grid-container--small" data-activity-map="listicle-product-article-body-ativafit-folding-exercise-bike" data-testid="listicle-product-container"><div class="listicle-product__grid-row"><div class="listicle-product__border hasProsCons"><div class="df flex-column items-stretch listicle-product__card-container flex-row-m"><div class="listicleProduct__pictureLink listicle-product__picture-link db db-m dn"><a href="https://www.amazon.com/ATIVAFIT-Magnetic-Stationary-Recumbent-Exercise/dp/B07L9TC976/?tag=1245309affordablebikes-20" rel="sponsored noopener noreferrer" data-testid="listicle-product-picture-link" target="_blank" data-product-id="rcpd14281" class="w-100"><picture class="" data-testid="picture"><source media="(min-width: 1240px)" srcSet="https://media-cldnry.s-nbcnews.com/image/upload/t_fit-260w,f_avif,q_auto:eco,dpr_2/rockcms/2024-01/AMAZON-90d873.jpg 2x, https://media-cldnry.s-nbcnews.com/image/upload/t_fit-260w,f_auto,q_auto:best/rockcms/2024-01/AMAZON-90d873.jpg 1x"/><source media="(min-width: 1000px)" srcSet="https://media-cldnry.s-nbcnews.com/image/upload/t_fit-260w,f_avif,q_auto:eco,dpr_2/rockcms/2024-01/AMAZON-90d873.jpg 2x, https://media-cldnry.s-nbcnews.com/image/upload/t_fit-260w,f_auto,q_auto:best/rockcms/2024-01/AMAZON-90d873.jpg 1x"/><source media="(min-width: 758px)" srcSet="https://media-cldnry.s-nbcnews.com/image/upload/t_fit-360w,f_avif,q_auto:eco,dpr_2/rockcms/2024-01/AMAZON-90d873.jpg 2x, https://media-cldnry.s-nbcnews.com/image/upload/t_fit-360w,f_auto,q_auto:best/rockcms/2024-01/AMAZON-90d873.jpg 1x"/><source media="(min-width: 320px)" srcSet="https://media-cldnry.s-nbcnews.com/image/upload/t_fit-320w,f_avif,q_auto:eco,dpr_2/rockcms/2024-01/AMAZON-90d873.jpg 2x, https://media-cldnry.s-nbcnews.com/image/upload/t_fit-320w,f_auto,q_auto:best/rockcms/2024-01/AMAZON-90d873.jpg 1x"/><img loading="lazy" src="https://media-cldnry.s-nbcnews.com/image/upload/t