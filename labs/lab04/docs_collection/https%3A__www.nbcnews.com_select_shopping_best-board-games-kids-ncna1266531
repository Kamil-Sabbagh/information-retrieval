    const { product: { id  } ,  } = props;
    return shouldShowComponent ? defaultId || id : undefined;
}"><div id="rcpd32841" class=""><div class="listicleProduct dn-print mv8 listicle-product__grid-container--small" data-activity-map="listicle-product-article-body-castle-panic-" data-testid="listicle-product-container"><div class="listicle-product__grid-row"><div class="listicle-product__border hasProsCons"><div class="df flex-column items-stretch listicle-product__card-container flex-row-m"><div class="listicleProduct__pictureLink listicle-product__picture-link db db-m dn"><a href="https://www.amazon.com/dp/B0B3LGW2KH?tag=1266531shoppingkidsboardgames-20" rel="sponsored noopener noreferrer" data-testid="listicle-product-picture-link" target="_blank" data-product-id="rcpd32841" class="w-100"><picture class="" data-testid="picture"><source media="(min-width: 1240px)" srcSet="https://media-cldnry.s-nbcnews.com/image/upload/t_fit-260w,f_avif,q_auto:eco,dpr_2/rockcms/2024-09/AMAZON-133f66.jpg 2x, https://media-cldnry.s-nbcnews.com/image/upload/t_fit-260w,f_auto,q_auto:best/rockcms/2024-09/AMAZON-133f66.jpg 1x"/><source media="(min-width: 1000px)" srcSet="https://media-cldnry.s-nbcnews.com/image/upload/t_fit-260w,f_avif,q_auto:eco,dpr_2/rockcms/2024-09/AMAZON-133f66.jpg 2x, https://media-cldnry.s-nbcnews.com/image/upload/t_fit-260w,f_auto,q_auto:best/rockcms/2024-09/AMAZON-133f66.jpg 1x"/><source media="(min-width: 758px)" srcSet="https://media-cldnry.s-nbcnews.com/image/upload/t_fit-360w,f_avif,q_auto:eco,dpr_2/rockcms/2024-09/AMAZON-133f66.jpg 2x, https://media-cldnry.s-nbcnews.com/image/upload/t_fit-360w,f_auto,q_auto:best/rockcms/2024-09/AMAZON-133f66.jpg 1x"/><source media="(min-width: 320px)" srcSet="https://media-cldnry.s-nbcnews.com/image/upload/t_fit-320w,f_avif,q_auto:eco,dpr_2/rockcms/2024-09/AMAZON-133f66.jpg 2x, https://media-cldnry.s-nbcnews.com/image/upload/t_fit-320w,f_auto,q_auto:best/rockcms/2024-09/AMAZON-133f66.jpg 1x"/><img loading="lazy" src="https://media-cldnry.s-nbcnews.com/image/upload/t_fit-320w,f_auto,q_auto:best/rockcms/2024-09/AMAZON-133f66.